{
    "name": "foonoo/foonoo",
    "description": "A static site generator. Ideal for generating blogs, docs and just about any kind of site you can think of.",
    "require": {
        "ekowabaka/clearice": "~3.0",
        "mustache/mustache": "~2.0",
        "erusev/parsedown" : "~1.0",
        "ntentan/honam": "~0.6.0",
        "ntentan/panie": "~0.7.0",
        "ntentan/kaikai": "@stable",
        "ntentan/utils": "~0.10",
        "symfony/yaml" : "~4.0",
        "smarty/smarty": "~3.0",
        "matthiasmullie/minify": "~1.3",
        "ext-dom": "*",
        "ext-libxml": "*",
        "ext-fileinfo": "*",
        "ext-pcntl": "*",
        "ext-imagick": "*",
        "ext-json": "*"
    },
    "require-dev" : {
        "phing/phing" : "@stable",
        "phpunit/phpunit": "@stable"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Ekow Abaka Ainooson",
            "email": "jainooson@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {"foonoo\\": "src/", "foonoo\\tests\\":  "tests/cases/"}
    },
    "bin" : ["bin/foonoo"]
}
